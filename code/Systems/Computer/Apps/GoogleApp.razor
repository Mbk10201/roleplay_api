@inherits WebPanel
@attribute [StyleSheet]
@implements IComputerApp

@namespace Mbk.RoleplayAPI.Systems.Computer

<root>
    This application is currently on maintenance.
</root>

@code {
    public string AppName => "Google";
    public string[] AppJobAccess => null;
    public long[] AppRoleId => null;
    public string Icon => "logos:google-icon";
    public string IconColor => "";
    public string WindowName => "Google";
    public bool Fullscreen => false;

    protected override void OnAfterTreeRender(bool firstTime)
    {
        base.OnAfterTreeRender(firstTime);

        if (firstTime)
        {
            Surface.Url = "https://google.com/";
            Surface.InBackgroundMode = false;
            Surface.HasKeyFocus = true;
        }
    }
}
